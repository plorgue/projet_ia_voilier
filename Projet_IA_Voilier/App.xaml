<Application x:Class="Projet_IA_Voilier.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Projet_IA_Voilier"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <SolidColorBrush x:Key="TextColorLight" Color="#222222"/>
        <SolidColorBrush x:Key="TextColorDark" Color="#ffffff"/>
        <SolidColorBrush x:Key="TextColorHint" Color="#999999"/>

        <SolidColorBrush x:Key="BackgroundLight" Color="#AAAAAA" />
        <SolidColorBrush x:Key="BackgroundDark" Color="#333333"/>
        
        <SolidColorBrush x:Key="redcolor" Color="Red"/>

        <GridLength x:Key="ColumnWidth_ControlPanel">270</GridLength>

    </Application.Resources>

    
    
</Application>
